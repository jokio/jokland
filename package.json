{
  "name": "jokland",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:ipfs": "ng build -c ipfs",
    "watch": "ng build --watch --configuration development",
    "dockerizex": "ng build && docker buildx build --platform linux/amd64 --push -t jokio/jokland:latest -f ./Dockerfile ./dist/jokland/browser",
    "dockerize": "ng build && docker build -t jokio/jokland:latest -f ./Dockerfile ./dist/jokland/browser",
    "test": "ng test",
    "dev:ssr": "ng run jokland:serve-ssr",
    "serve:ssr": "node dist/jokland/server/main.js",
    "build:ssr": "ng build && ng run jokland:server",
    "prerender": "ng run jokland:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/platform-server": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@angular/service-worker": "~13.2.0",
    "@capacitor/core": "^3.4.0",
    "@capacitor/share": "^1.1.1",
    "@ionic/angular": "^6.0.3",
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/onboarding": "^1.0.1",
    "@nguniversal/express-engine": "^13.0.2",
    "ethers": "^5.5.3",
    "express": "^4.15.2",
    "merge-images": "^2.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@capacitor/cli": "^3.4.0",
    "@ionic/angular-toolkit": "latest",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/merge-images": "^1.2.1",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
